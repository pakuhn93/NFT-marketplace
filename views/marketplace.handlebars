<div class="div-container">
    <br>
    <h2>Marketplace</h2>
    <hr>
    <table class="maketplace-container">
        <tr>
            <th>Option</th>
            <th>Name</th>
            <th>Description</th>
            <th>Price</th>
            <th>Stock</th>
            <th>Category</th>
        </tr>
        <tr>
            {{#each items as |item|}}
            <td><button id="editPost" data-id="{{id}}">View</button></td>
            <td>{{item.item_name}}</td>
            <td>{{item.description}}</td>
            <td>{{item.price}}</td>
            <td>{{item.stock}}</td>
            <td>{{item.category_id}}</td>
        </tr>
        {{/each}}
    </table>
</div>

